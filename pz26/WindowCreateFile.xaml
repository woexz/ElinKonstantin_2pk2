<Window x:Class="Wpfpz26.Window1"
        MaxHeight="300"
        MinHeight="300"
        MaxWidth="300"
        MinWidth="300"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpfpz26"
        mc:Ignorable="d"
        Title="Window1" Height="300" Width="300">
    <Grid>
        <ListBox>
            <TextBlock VerticalAlignment="Top">Введите название файла для поиска</TextBlock>
            <TextBox x:Name="NameFile" Width="280"/>
            
        </ListBox>
        <Button x:Name="CreateFileOk" VerticalAlignment="Bottom" Click="CreateFileOk_Click">Ок</Button>
        </Grid>
</Window>
